<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TestDemo.MainContentControl">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>

		<Button x:Name="btnBack" Content="Назад" Click="BackButton_Click_2"  IsEnabled="False"/>

		<StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Center">
			<Button Content="Список инвентаря" Click="InventoryButton_Click"/>
			<Button Content="Категории инвентаря" Click="CategoriesButton_Click_1"/>
		</StackPanel>

		<ContentControl Grid.Row="1" x:Name="ContentControl"/>
	</Grid>
</UserControl>
