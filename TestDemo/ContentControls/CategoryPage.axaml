<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="using:TestDemo.Data"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TestDemo.CategoryPage"
			 x:DataType="local:Category">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="*"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>

		<DataGrid IsReadOnly="True" Grid.Row="0" AutoGenerateColumns="False" ItemsSource="{Binding}" Name="DataGridCategory">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Название категории" Binding="{Binding Name}"/>
			</DataGrid.Columns>
		</DataGrid>
		
		
	</Grid>
</UserControl>
